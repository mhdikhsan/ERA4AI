<!-- =================== VULNERABILITIES =================== -->

<!-- Data Vulnerabilities -->
<rdf:Description rdf:about="http://example.org/oes#Vuln_DataBreach">
  <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/era4ai#PrivacyVulnerability"/>
  <era4ai:hasName>Personal Data Breach Vulnerability</era4ai:hasName>
  <era4ai:hasDescription>Risk of unauthorized access to candidate personal information</era4ai:hasDescription>
  <era4ai:vulnerabilityScore>8.5</era4ai:vulnerabilityScore>
  <era4ai:exploitabilityLevel>High</era4ai:exploitabilityLevel>
  <era4ai:exposesAsset rdf:resource="http://example.org/oes#Asset_CandidateData"/>
</rdf:Description>

<rdf:Description rdf:about="http://example.org/oes#Vuln_QuestionLeakage">
  <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/era4ai#DataVulnerability"/>
  <era4ai:hasName>Question Bank Leakage</era4ai:hasName>
  <era4ai:hasDescription>Risk of exam questions being exposed before examinations</era4ai:hasDescription>
  <era4ai:vulnerabilityScore>9.0</era4ai:vulnerabilityScore>
  <era4ai:exploitabilityLevel>High</era4ai:exploitabilityLevel>
  <era4ai:exposesAsset rdf:resource="http://example.org/oes#Asset_QuestionBank"/>
</rdf:Description>

<!-- Model Vulnerabilities -->
<rdf:Description rdf:about="http://example.org/oes#Vuln_FacialSpoofing">
  <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/era4ai#AdversarialVulnerability"/>
  <era4ai:hasName>Facial Recognition Spoofing</era4ai:hasName>
  <era4ai:hasDescription>Vulnerability to photo/video spoofing attacks against facial recognition</era4ai:hasDescription>
  <era4ai:vulnerabilityScore>7.0</era4ai:vulnerabilityScore>
  <era4ai:exploitabilityLevel>Medium</era4ai:exploitabilityLevel>
  <era4ai:exposesAsset rdf:resource="http://example.org/oes#Asset_FacialRecModel"/>
</rdf:Description>

<rdf:Description rdf:about="http://example.org/oes#Vuln_FalsePositives">
  <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/era4ai#RobustnessVulnerability"/>
  <era4ai:hasName>Cheating Detection False Positives</era4ai:hasName>
  <era4ai:hasDescription>AI model incorrectly flagging legitimate behaviors as cheating</era4ai:hasDescription>
  <era4ai:vulnerabilityScore>6.5</era4ai:vulnerabilityScore>
  <era4ai:exploitabilityLevel>Medium</era4ai:exploitabilityLevel>
  <era4ai:exposesAsset rdf:resource="http://example.org/oes#Asset_CheatingDetModel"/>
</rdf:Description>

<!-- System Vulnerabilities -->
<rdf:Description rdf:about="http://example.org/oes#Vuln_SystemOverload">
  <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/era4ai#SystemVulnerability"/>
  <era4ai:hasName>System Performance Degradation</era4ai:hasName>
  <era4ai:hasDescription>System failure under peak load of 100,000 concurrent users</era4ai:hasDescription>
  <era4ai:vulnerabilityScore>7.5</era4ai:vulnerabilityScore>
  <era4ai:exploitabilityLevel>Medium</era4ai:exploitabilityLevel>
  <era4ai:exposesAsset rdf:resource="http://example.org/oes#Asset_ExamPlatform"/>
</rdf:Description>

<rdf:Description rdf:about="http://example.org/oes#Vuln_DatabaseInjection">
  <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/era4ai#SystemVulnerability"/>
  <era4ai:hasName>SQL Injection Vulnerability</era4ai:hasName>
  <era4ai:hasDescription>Risk of malicious SQL injection attacks on database</era4ai:hasDescription>
  <era4ai:vulnerabilityScore>8.0</era4ai:vulnerabilityScore>
  <era4ai:exploitabilityLevel>High</era4ai:exploitabilityLevel>
  <era4ai:exposesAsset rdf:resource="http://example.org/oes#Asset_Database"/>
</rdf:Description>

<!-- Process Vulnerabilities -->
<rdf:Description rdf:about="http://example.org/oes#Vuln_WeakAuthentication">
  <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/era4ai#ProcessVulnerability"/>
  <era4ai:hasName>Weak Authentication Process</era4ai:hasName>
  <era4ai:hasDescription>Insufficient authentication mechanisms for system access</era4ai:hasDescription>
  <era4ai:vulnerabilityScore>6.0</era4ai:vulnerabilityScore>
  <era4ai:exploitabilityLevel>Medium</era4ai:exploitabilityLevel>
</rdf:Description>

<!-- Human Vulnerabilities -->
<rdf:Description rdf:about="http://example.org/oes#Vuln_SocialEngineering">
  <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/era4ai#HumanVulnerability"/>
