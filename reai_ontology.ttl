@prefix : <http://www.semanticweb.org/ontologies/era4ai#> .
@prefix airo: <https://w3id.org/airo#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/2001/XMLSchema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/ontologies/era4ai#> .

<http://www.semanticweb.org/ontologies/era4ai> rdf:type owl:Ontology ;
    owl:imports <https://w3id.org/airo> ;
    rdfs:label "ERA4AI: Risk-aware Requirements Engineering for AI Systems"@en ;
    rdfs:comment "An ontology integrating Goal-Oriented Requirements Engineering (GORE) with AIRO (AI Risk Ontology) for comprehensive risk modeling in AI systems. Extends AIRO with GORE concepts and AI-specific risk patterns."@en ;
    owl:versionInfo "1.1" .

#################################################################
#    Object Properties
#################################################################

### Goal-related Properties
:refinesGoal rdf:type owl:ObjectProperty ;
    rdfs:domain :Goal ;
    rdfs:range :Goal ;
    rdfs:label "refines goal"@en ;
    rdfs:comment "Relates a goal to its refined sub-goal"@en .

:operationalizedBy rdf:type owl:ObjectProperty ;
    rdfs:domain :Goal ;
    rdfs:range :Task ;
    rdfs:label "operationalized by"@en ;
    rdfs:comment "Links a goal to the task that operationalizes it"@en .

:contributesPositivelyTo rdf:type owl:ObjectProperty ;
    rdfs:domain :Softgoal ;
    rdfs:range :Softgoal ;
    rdfs:label "contributes positively to"@en .

:contributesNegativelyTo rdf:type owl:ObjectProperty ;
    rdfs:domain :Softgoal ;
    rdfs:range :Softgoal ;
    rdfs:label "contributes negatively to"@en .

:conflictsWith rdf:type owl:ObjectProperty ;
    rdfs:domain :Goal ;
    rdfs:range :Goal ;
    rdfs:label "conflicts with"@en .

### Responsibility Properties
:assignedTo rdf:type owl:ObjectProperty ;
    rdfs:domain :Task ;
    rdfs:range :Agent ;
    rdfs:label "assigned to"@en .

:hasRole rdf:type owl:ObjectProperty ;
    rdfs:domain :Actor ;
    rdfs:range :Role ;
    rdfs:label "has role"@en .

:dependsOn rdf:type owl:ObjectProperty ;
    rdfs:label "depends on"@en ;
    rdfs:comment "General dependency relationship"@en .

### Risk-related Properties (AIRO-aligned)
:obstructs rdf:type owl:ObjectProperty ;
    rdfs:domain :Obstacle ;
    rdfs:range :Goal ;
    rdfs:label "obstructs"@en ;
    rdfs:comment "Links an obstacle to the goal it prevents from being satisfied"@en .

:manifestsAs rdf:type owl:ObjectProperty ;
    rdfs:domain :RiskSource ;
    rdfs:range :Obstacle ;
    rdfs:label "manifests as"@en ;
    rdfs:comment "Links a risk source to the obstacle it creates"@en .

:causedBy rdf:type owl:ObjectProperty ;
    rdfs:domain :Risk ;
    rdfs:range :RiskSource ;
    rdfs:label "caused by"@en ;
    rdfs:comment "Links a risk to its originating risk source"@en .

:hasConsequence rdf:type owl:ObjectProperty ;
    rdfs:domain :Risk ;
    rdfs:range :Consequence ;
    rdfs:label "has consequence"@en ;
    rdfs:comment "Relates a risk to its potential consequences"@en .

:threatens rdf:type owl:ObjectProperty ;
    rdfs:domain :Risk ;
    rdfs:range :Asset ;
    rdfs:label "threatens"@en ;
    rdfs:comment "Indicates which assets are at risk"@en .

:mitigatedBy rdf:type owl:ObjectProperty ;
    rdfs:domain :Risk ;
    rdfs:range :RiskControl ;
    rdfs:label "mitigated by"@en ;
    rdfs:comment "Links a risk to risk controls that address it"@en .

:occursIn rdf:type owl:ObjectProperty ;
    rdfs:domain :Risk ;
    rdfs:range :LifecycleStage ;
    rdfs:label "occurs in"@en ;
    rdfs:comment "Indicates when in the AI lifecycle the risk may manifest"@en .

:hasImpact rdf:type owl:ObjectProperty ;
    rdfs:domain :Consequence ;
    rdfs:range :Impact ;
    rdfs:label "has impact"@en ;
    rdfs:comment "Links consequences to their resulting impacts"@en .

