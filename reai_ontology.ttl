#################################################################
#    Classes - GORE Taxonomy
#################################################################

### 1. Goal Types
:Goal rdf:type owl:Class ;
    rdfs:label "Goal"@en ;
    rdfs:comment "A desired state or objective in the system"@en .

:FunctionalGoal rdf:type owl:Class ;
    rdfs:subClassOf :Goal ;
    rdfs:label "Functional Goal"@en ;
    rdfs:comment "Describes what the system must do"@en .

:Softgoal rdf:type owl:Class ;
    rdfs:subClassOf :Goal ;
    rdfs:label "Softgoal (Non-Functional Goal)"@en ;
    rdfs:comment "Describes qualities or constraints with subjective satisfaction"@en .

:BehavioralGoal rdf:type owl:Class ;
    rdfs:subClassOf :Goal ;
    rdfs:label "Behavioral Goal"@en ;
    rdfs:comment "Describes system behavior over time"@en .

:DomainGoal rdf:type owl:Class ;
    rdfs:subClassOf :Goal ;
    rdfs:label "Domain/Context Goal"@en ;
    rdfs:comment "Environmental rules or domain-specific assumptions"@en .

:ValueGoal rdf:type owl:Class ;
    rdfs:subClassOf :Goal ;
    rdfs:label "Value/Ethical Goal"@en ;
    rdfs:comment "Human values or ethical principles"@en .

### 2. Goal Refinement
:StrategicGoal rdf:type owl:Class ;
    rdfs:subClassOf :Goal ;
    rdfs:label "Strategic Goal"@en ;
    rdfs:comment "High-level business motivations"@en .

:TacticalGoal rdf:type owl:Class ;
    rdfs:subClassOf :Goal ;
    rdfs:label "Tactical Goal"@en ;
    rdfs:comment "Mid-level goals linking strategy to operations"@en .

:OperationalGoal rdf:type owl:Class ;
    rdfs:subClassOf :Goal ;
    rdfs:label "Operational Goal"@en ;
    rdfs:comment "Concrete, implementable goals"@en .

:ANDRefinement rdf:type owl:Class ;
    rdfs:subClassOf :Goal ;
    rdfs:label "AND Refinement"@en ;
    rdfs:comment "All subgoals are required to satisfy the parent goal"@en .

:ORRefinement rdf:type owl:Class ;
    rdfs:subClassOf :Goal ;
    rdfs:label "OR Refinement"@en ;
    rdfs:comment "Alternative ways to satisfy a goal"@en .

### 3. Responsibility/Agency
:Agent rdf:type owl:Class ;
    rdfs:label "Agent"@en ;
    rdfs:comment "System or AI module assigned tasks"@en .

:Actor rdf:type owl:Class ;
    rdfs:label "Actor"@en ;
    rdfs:comment "Human stakeholder with goals"@en .

:Role rdf:type owl:Class ;
    rdfs:label "Role"@en ;
    rdfs:comment "Abstract responsibility label"@en .

### 4. Obstacles (GORE-Risk Bridge)
:Obstacle rdf:type owl:Class ;
    rdfs:label "Obstacle"@en ;
    rdfs:comment "Situation preventing goal satisfaction - bridges GORE and Risk concepts"@en .

:AntiGoal rdf:type owl:Class ;
    rdfs:subClassOf :Obstacle ;
    rdfs:label "Anti-Goal"@en ;
    rdfs:comment "Malicious actor's objective"@en .

### 5. Means-Ends
:Task rdf:type owl:Class ;
    rdfs:label "Task"@en ;
    rdfs:comment "Concrete action that achieves goals"@en .

:Resource rdf:type owl:Class ;
    rdfs:label "Resource"@en ;
    rdfs:comment "Required or produced artifact"@en .

:Plan rdf:type owl:Class ;
    rdfs:label "Plan"@en ;
    rdfs:comment "Strategy or workflow for achieving goals"@en .

#################################################################
#    Classes - Risk Taxonomy
#################################################################

### Core Risk Concepts (Extending AIRO)
:Risk rdf:type owl:Class ;
    rdfs:subClassOf airo:Risk ;
    rdfs:label "AI System Risk"@en ;
    rdfs:comment "AI-specific extension of AIRO Risk concept - potential for unwanted outcomes in AI systems caused by the combination of risk sources exploiting vulnerabilities"@en .

:RiskSource rdf:type owl:Class ;
    rdfs:subClassOf airo:RiskSource ;
    rdfs:label "AI Risk Source"@en ;
    rdfs:comment "AI-specific extension of AIRO RiskSource - factors that trigger or generate risk in AI systems"@en .

:Vulnerability rdf:type owl:Class ;
    rdfs:subClassOf airo:Vulnerability ;
    rdfs:label "AI Vulnerability"@en ;
    rdfs:comment "AI-specific extension of AIRO Vulnerability - weaknesses or flaws in AI assets that can be exploited by risk sources"@en .

:Consequence rdf:type owl:Class ;
    rdfs:subClassOf airo:Consequence ;
    rdfs:label "AI Risk Consequence"@en ;
    rdfs:comment "AI-specific extension of AIRO Consequence - direct effects of AI risk realization"@en .

:RiskControl rdf:type owl:Class ;
    rdfs:subClassOf airo:RiskControl ;
    rdfs:label "AI Risk Control"@en ;
    rdfs:comment "AI-specific extension of AIRO RiskControl - measures implemented to modify AI risks"@en .

:Impact rdf:type owl:Class ;
    rdfs:subClassOf airo:Impact ;
    rdfs:label "AI Risk Impact"@en ;
    rdfs:comment "AI-specific extension of AIRO Impact - types of harm caused by AI risks"@en .

:Asset rdf:type owl:Class ;
    rdfs:subClassOf airo:Asset ;
    rdfs:label "AI Asset"@en ;
    rdfs:comment "AI-specific extension of AIRO Asset - valuable objects in AI systems vulnerable to risk"@en .

### ERA4AI-Specific Risk Concepts
:AIRiskScenario rdf:type owl:Class ;
    rdfs:label "AI Risk Scenario"@en ;
    rdfs:comment "Specific scenario describing how AI risks can manifest in particular contexts"@en .

:RiskPattern rdf:type owl:Class ;
    rdfs:label "AI Risk Pattern"@en ;
    rdfs:comment "Common recurring patterns of risk in AI systems"@en .

:MitigationGoal rdf:type owl:Class ;
    rdfs:subClassOf :Goal ;
    rdfs:label "Mitigation Goal"@en ;
    rdfs:comment "GORE-specific goal representing risk control objectives - bridges AIRO controls with GORE goals"@en .

