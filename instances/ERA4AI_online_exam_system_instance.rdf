<!--                     ONLINE EXAMINATION SYSTEM                     -->
<!--                     ERA4AI ONTOLOGY INSTANCES                     -->
<!-- ================================================================== -->

<!-- =================== STRATEGIC GOALS =================== -->

<!-- Main System Goal -->
<rdf:Description rdf:about="http://example.org/oes#G_ModernizeExamination">
  <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/era4ai#StrategicGoal"/>
  <era4ai:hasName>Modernize Standardized Testing</era4ai:hasName>
  <era4ai:hasDescription>Replace paper-based exams with a secure, scalable digital platform for government organization</era4ai:hasDescription>
</rdf:Description>

<!-- Security Goals -->
<rdf:Description rdf:about="http://example.org/oes#G_SecureExamination">
  <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/era4ai#ValueGoal"/>
  <era4ai:hasName>Ensure Exam Security and Integrity</era4ai:hasName>
  <era4ai:hasDescription>Maintain the highest level of security and prevent cheating during examinations</era4ai:hasDescription>
  <era4ai:refinesGoal rdf:resource="http://example.org/oes#G_ModernizeExamination"/>
</rdf:Description>

<rdf:Description rdf:about="http://example.org/oes#G_DataPrivacy">
  <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/era4ai#ValueGoal"/>
  <era4ai:hasName>Protect Candidate Data Privacy</era4ai:hasName>
  <era4ai:hasDescription>Ensure GDPR compliance and secure handling of personal data</era4ai:hasDescription>
  <era4ai:refinesGoal rdf:resource="http://example.org/oes#G_ModernizeExamination"/>
</rdf:Description>

<!-- Performance Goals -->
<rdf:Description rdf:about="http://example.org/oes#G_Scalability">
  <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/era4ai#Softgoal"/>
  <era4ai:hasName>Support 100,000 Concurrent Users</era4ai:hasName>
  <era4ai:hasDescription>System must handle peak loads of up to 100,000 simultaneous users</era4ai:hasDescription>
  <era4ai:hasSatisfactionLevel>High</era4ai:hasSatisfactionLevel>
  <era4ai:refinesGoal rdf:resource="http://example.org/oes#G_ModernizeExamination"/>
</rdf:Description>

<rdf:Description rdf:about="http://example.org/oes#G_ResponseTime">
  <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/era4ai#Softgoal"/>
  <era4ai:hasName>Maintain Fast Response Times</era4ai:hasName>
  <era4ai:hasDescription>Database queries within 2 seconds, server response under 3 seconds</era4ai:hasDescription>
  <era4ai:hasSatisfactionLevel>High</era4ai:hasSatisfactionLevel>
</rdf:Description>

<!-- AI-Specific Goals -->
<rdf:Description rdf:about="http://example.org/oes#G_AIProctoring">
  <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/era4ai#FunctionalGoal"/>
  <era4ai:hasName>Implement AI-Powered Proctoring</era4ai:hasName>
  <era4ai:hasDescription>Use facial recognition and cheating detection for exam integrity</era4ai:hasDescription>
  <era4ai:refinesGoal rdf:resource="http://example.org/oes#G_SecureExamination"/>
</rdf:Description>

